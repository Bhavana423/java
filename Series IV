import java.util.Scanner;
public class Series4
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        System.out.printf("0.5 ");
        double s=0.5;
        for(int i=0;i<a;i++){
            s=s*3;
            System.out.printf("%.1f ",s );
        }
        
    }
}
