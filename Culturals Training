import java.util.*;
public class Football
{
    public static void main(String args[])
    {
       Scanner sc=new Scanner(System.in);
       int a=sc.nextInt();
       int b=sc.nextInt();
       int c=a/b;
       int e=c*b;
       int d=a-e;
       System.out.println(c);
       System.out.println(d);
    }
}
